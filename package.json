{
    "name": "first-extension",
    "displayName": "First Extension",
    "description": "",
    "version": "0.0.1",
    "publisher": "demo",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:saveByAuthor",
        "onCommand:setUser"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "setUser",
                "title": "set user"
            }
        ],
        "keybindings": [{          
            "command": "saveByAuthor", 
            "key": "ctrl+s"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
